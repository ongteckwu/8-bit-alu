//8-bit compare module
module compare (
    input alufn21[2],
    input z,
    input n,
    input v,
    output out[8]
  ) {
  always {
    //choosing the operation based on the values of alufn[2:1]
    case (alufn21[1:0]) {
      b01:
        out[0] = z;
      b10:
        out[0] = n^v;
      b11:
        out[0] = z|(n^v);
      default:
        out[0] = z;
  }
    out[7:1] = 7b0;
  }
}
